script/source = "extends KinematicBody2D

# class member variables go here, for example:
# var a = 2
# var b = \"textvar\"
export(int) var damage = 10
export var velocity = Vector2()
export(int) var speed = 100
export(float) var smoothness = 0.1
export var direction = 1
onready var sprite = get_node(\"Sprite\")
var placing = true

func _ready():
	#nur innerhalb der map
	if(get_pos().x < 10 or get_pos().y < 10 or get_pos().x > 1270 or get_pos().y > 970):
		queue_free()


func _on_Area2D_body_enter( body ):
	if(body.is_in_group(\"hero\")):		
		body.getDamage(damage)
		queue_free()
	if(placing && body.is_in_group(\"wall\")):
			queue_free()


func _fixed_process(delta):	
	if(is_colliding()):
		direction = direction * (-1)
		
	velocity.y = lerp(velocity.y, speed * direction, smoothness)
	var motion = velocity * delta
	move(motion)
	
func start():
	placing = false
	set_fixed_process(true)
	
	
	
	
	
	
	"

